{
  "packages": [
    "packages/*",
    "examples/*",
    "apps/*"
  ],
  "changelog": {
    "labels": {
      "enhancement": ":rocket: Enhancement",
      "bug": ":bug: Bug Fix",
      "Type: Documentation ": ":memo: Documentation",
      "Type: Feature": ":rocket: Type: Feature",
      "Type: Bug": ":bug: Type: Bug",
      "Type: Breaking Change": ":boom: Type: Breaking Change"
    },
    "cacheDir": ".changelog"
  },
  "npmClient": "yarn",
  "useWorkspaces": true,
  "version": "5.2.1"
}
